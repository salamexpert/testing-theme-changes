  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="{{ settings.color_button }}">
  {%- if settings.favicon != blank -%}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {%- endif -%}
{%- if request.path contains '/tagged/' and current_tags.size > 0 -%}
  <meta name="description" content="Explore BubbleBum blog posts tagged with {{ current_tags.first }}. Discover travel tips, car seat safety guides, and parenting hacks for stress-free family journeys.">
{%- elsif request.path == '/collections/all' or request.path == '/en-ie/collections/all' or request.path == '/en-de/collections/all' -%}
  <meta name="description" content="Shop all BubbleBum collections in one place. Discover travel car booster seats, kids’ travel accessories, and safety essentials for stress-free journeys.">
{%- elsif page_description -%}
  <meta name="description" content="{{ page_description | escape }}">
{%- endif -%}
  {%- if canonical_url != blank  -%}
    <link rel="canonical" href="{{ canonical_url }}">
  {%- endif -%}
  {%- capture seo_title -%}
    {%- if request.page_type == 'search' and search.performed == true -%}
      {{ 'general.search.heading' | t: count: search.results_count }}: {{ 'general.search.results_with_count' | t: terms: search.terms, count: search.results_count }}
    {%- else -%}
      {{ page_title }}
    {%- endif -%}
    {%- if current_tags -%}
      {%- assign meta_tags = current_tags | join: ', ' -%} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
    {%- endif -%}
    {%- if current_page != 1 -%}
      &ndash; {{ 'general.meta.page' | t: page: current_page }}
    {%- endif -%}
    {%- assign escaped_page_title = page_title | escape -%}
    {%- unless escaped_page_title contains shop.name -%}
      &ndash; {{ shop.name }}
    {%- endunless -%}
  {%- endcapture -%}
  
{%- if request.path contains '/blogs/' -%}
  {%- if current_tags and current_tags.size > 0 -%}
    <title>{{ page_title | strip }} – {{ current_tags.first }}</title>
  {%- else -%}
    <title>{{ page_title }} - BubbleBum</title>
  {%- endif -%}

{%- elsif request.path == '/' or request.path contains '/products/' or request.path contains '/pages/' -%}
  <title>{{ page_title | strip }}</title>

{%- elsif request.path contains '/en-de' or request.path contains '/en-ie' -%}
  {%- if request.path == '/en-de' or request.path == '/en-ie' -%}
    <title>{{ page_title | strip }}</title>
  {%- elsif request.path contains '/products/' or request.path contains '/pages/' -%}
    <title>{{ page_title | strip }}</title>
  {%- else -%}
    <title>{{ seo_title | strip }}</title>
  {%- endif -%}

{%- else -%}
  <title>{{ seo_title | strip }}</title>
{%- endif -%}
 
